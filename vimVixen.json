{

    "keymaps": {

        "0": {
            "type": "scroll.home"
        },
        ":": {
            "type": "command.show"
        },
        "$": {
            "type": "scroll.end"
        },
        "/": {
            "type": "find.start"
        },
        "'": {
            "type": "mark.jump.prefix"
        },
        "!d": {
            "type": "tabs.close.force"
        },
        "[[": {
            "type": "navigate.link.prev"
        },
        "]]": {
            "type": "navigate.link.next"
        },
        "b": {
            "type": "command.show.buffer"
        },
        "d": {
            "type": "tabs.close"
        },
        "D": {
            "type": "tabs.close.right"
        },
        "f": {
            "type": "follow.start", "newTab": false
        },
        "F": {
            "type": "follow.start", "newTab": true
        },
        "G": {
            "type": "scroll.bottom"
        },
        "h": {
            "type": "scroll.horizonally", "count": -1
        },
        "H": {
            "type": "navigate.history.prev"
        },
        "j": {
            "type": "scroll.vertically", "count": 1
        },
        "J": {
            "type": "tabs.next", "count": 1
        },
        "k": {
            "type": "scroll.vertically", "count": -1
        },
        "K": {
            "type": "tabs.prev", "count": 1
        },
        "l": {
            "type": "scroll.horizonally", "count": 1
        },
        "L": {
            "type": "navigate.history.next"
        },
        "m": {
            "type": "mark.set.prefix"
        },
        "n": {
            "type": "find.next"
        },
        "N": {
            "type": "find.prev"
        },
        "o": {
            "type": "command.show.open", "alter": false
        },
        "O": {
            "type": "command.show.open", "alter": true
        },
        "p": {
            "type": "urls.paste", "newTab": false
        },
        "P": {
            "type": "urls.paste", "newTab": true
        },
        "r": {
            "type": "tabs.reload", "cache": false
        },
        "R": {
            "type": "tabs.reload", "cache": true
        },
        "t": {
            "type": "command.show.tabopen", "alter": false
        },
        "T": {
            "type": "command.show.tabopen", "alter": true
        },
        "u": {
            "type": "tabs.reopen"
        },
        "w": {
            "type": "command.show.winopen", "alter": false
        },
        "W": {
            "type": "command.show.winopen", "alter": true
        },
        "y": {
            "type": "urls.yank"
        },
        "g0": {
            "type": "tabs.first"
        },
        "g$": {
            "type": "tabs.last"
        },
        "gg": {
            "type": "scroll.top"
        },
        "gh": {
            "type": "page.home"
        },
        "gH": {
            "type": "page.home", "newTab": true
        },
        "gt": {
            "type": "tabs.next"
        },
        "gT": {
            "type": "tabs.prev"
        },
        "gu": {
            "type": "navigate.parent"
        },
        "gU": {
            "type": "navigate.root"
        },
        "gi": {
            "type": "focus.input"
        },
        "zp": {
            "type": "tabs.pin.toggle"
        },
        "zd": {
            "type": "tabs.duplicate"
        },
        "zi": {
            "type": "zoom.in"
        },
        "zo": {
            "type": "zoom.out"
        },
        "zz": {
            "type": "zoom.neutral"
        },
        "<S-Esc>": {
            "type": "addon.toggle.enabled"
        },
        "<C-6>": {
            "type": "tabs.prevsel"
        },
        "<C-U>": {
            "type": "scroll.pages", "count": -0.5
        },
        "<C-D>": {
            "type": "scroll.pages", "count": 0.5
        },
        "<C-B>": {
            "type": "scroll.pages", "count": -1
        },
        "<C-F>": {
            "type": "scroll.pages", "count": 1
        }

    },
    "search": {

        "default": "google",
        "engines": {
            "google": "https://google.com/search?q={}",
            "yahoo": "https://search.yahoo.com/search?p={}",
            "bing": "https://www.bing.com/search?q={}",
            "duckduckgo": "https://duckduckgo.com/?q={}",
            "twitter": "https://twitter.com/search?q={}",
            "wikipedia": "https://en.wikipedia.org/w/index.php?search={}"
        }
    },
    "blacklist": [
        "mail.google.com",
        "inbox.google.com",
        "jupyter.ad.edrev.net"
    ],
    "properties": {
        "hintchars": "abcdefghijklmnopqrstuvwxyz",
        "smoothscroll": false
    }
}

